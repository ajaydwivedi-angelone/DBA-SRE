              "rawSql": "declare @publisher sysname;\r\ndeclare @publication nvarchar(1000);\r\ndeclare @subscription nvarchar(1000);\r\ndeclare @latency int;\r\n\r\nif len('$publisher') > 0\r\n  set @publisher = '$publisher';\r\nif len('$publication') > 0\r\n  set @publication = '$publication'\r\nif len('$subscription') > 0\r\n  set @subscription = '$subscription';\r\nif len('$latency') > 0\r\n  set @latency = convert(int,'$latency')\r\n  \r\n--select [from] = dbo.utc2local($__timeFrom()), [to] = dbo.utc2local($__timeTo())\r\n\r\n;with cte_subs as (\r\n\tselect * from [196.1.115.228].DBA_Admin.dbo.repl_pub_subs as sub\r\n\twhere (@publisher is null or sub.publisher like ('%'+@publisher+'%'))\r\n\tand (@publication is null or sub.publication_display_name like ('%'+@publication+'%'))\r\n\tand (@subscription is null or sub.subscription_display_name like ('%'+@subscription+'%'))\r\n)\r\nselect dbo.local2utc(publisher_commit) as time, [metric] = [publisher]+'->'+publication_display_name+' => '+subscription_display_name, value = overall_latency\r\nfrom [196.1.115.228].DBA_Admin.dbo.[repl_token_history] h join cte_subs sub on sub.repl_id = h.repl_id \r\nwhere (publisher_commit between dbo.utc2local($__timeFrom()) and dbo.utc2local($__timeTo()))\r\norder by time",
