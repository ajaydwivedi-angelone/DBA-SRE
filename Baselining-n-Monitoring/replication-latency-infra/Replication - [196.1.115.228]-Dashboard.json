          "rawSql": "declare @publisher sysname;\r\ndeclare @publication nvarchar(1000);\r\ndeclare @subscription nvarchar(1000);\r\ndeclare @latency int;\r\n\r\nif len('$publisher') > 0\r\n  set @publisher = '$publisher';\r\nif len('$publication') > 0\r\n  set @publication = '$publication'\r\nif len('$subscription') > 0\r\n  set @subscription = '$subscription';\r\nif len('$latency') > 0\r\n  set @latency = convert(int,'$latency')\r\n\r\n;with cte_latency as (\r\n  select h.publisher, publication_display_name, subscription_display_name, last_token_time, last_token_latency = last_token_latency_seconds\r\n  \t\t,current_approx_latency = current_latency_seconds\r\n  from [196.1.115.228].DBA_Admin.dbo.vw_repl_latency h\r\n  where 1 = 1\r\n  and (@publisher is null or h.publisher like ('%'+@publisher+'%'))\r\n  and (@publication is null or h.publication_display_name like ('%'+@publication+'%'))\r\n  and (@subscription is null or h.subscription_display_name like ('%'+@subscription+'%'))\r\n)\r\nselect *\r\nfrom cte_latency\r\nwhere (@latency is null or current_approx_latency >= @latency)",
